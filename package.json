{
	"name": "home-automation",
	"version": "1.0.0",
	"main": "app/server/app.js",
	"repository": "https://github.com/SanderRonde/home-automation",
	"author": "Sander Ronde <awsdfgvhbjn@gmail.com>",
	"license": "MIT",
	"private": true,
	"nodemonConfig": {
		"ext": "js",
		"exec": "node --experimental-modules --enable-source-maps --trace-warnings -r ts-node/register/transpile-only app/server/app.ts --http 1272 --https 1273 -v* --log-secrets",
		"watch": [
			"app/server/"
		]
	},
	"scripts": {
		"compile:server": "tsc -p app/server/tsconfig.json",
		"compile:client": "tsc -p app/client/tsconfig.json",
		"compile:ai": "tsc -p ai/tsconfig.json",
		"bundle:keyval": "esbuild --bundle app/client/keyval/keyval.ts --outfile=app/client/keyval/keyval.bundle.js --format=iife --minify",
		"bundle:home-detector": "esbuild --bundle app/client/home-detector/home-detector.ts --outfile=app/client/home-detector/home-detector.bundle.js --format=iife --minify",
		"bundle:rgb": "esbuild --bundle app/client/rgb/rgb.ts --outfile=app/client/rgb/rgb.bundle.js --format=iife --minify",
		"bundle:info-screen": "esbuild --bundle app/client/info-screen/info-screen.ts --outfile=app/client/info-screen/info-screen.bundle.js --format=iife --minify",
		"bundle:oauth": "esbuild --bundle app/client/oauth/oauth.tsx --outfile=app/client/oauth/oauth.bundle.js --format=iife --minify",
		"bundle": "yarn bundle:keyval && yarn bundle:home-detector && yarn bundle:rgb && yarn bundle:info-screen && yarn bundle:oauth",
		"watch:server": "tsc -w -p app/server/tsconfig.json",
		"watch:client": "tsc -w -p app/client/tsconfig.json",
		"watch:ai": "tsc -w -p ai/tsconfig.json",
		"html-typings": "html-typings -i ai/annotator/components/annotator-instance/annotator-instance.html.ts -o ai/annotator/components/annotator-instance/annotator-instance-querymap.d.ts -e",
		"compile": "yarn html-typings && yarn compile:server && yarn compile:client && yarn compile:ai && yarn bundle",
		"watch": "tsc -w -p app/server/tsconfig.json",
		"pretty": "pretty-quick",
		"eslint": "eslint --fix \"./app/**/*.ts\"",
		"start": "node --experimental-modules --enable-source-maps --trace-warnings -r ts-node/register/transpile-only app/server/app.ts --http 1272 --https 1273 -v* --name 1000 --scripts /home/sander/scripts --color -i"
	},
	"devDependencies": {
		"@types/body-parser": "^1.17.0",
		"@types/chalk": "^2.2.0",
		"@types/cookie-parser": "^1.4.1",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.0",
		"@types/express-oauth-server": "^2.0.3",
		"@types/fs-extra": "^7.0.0",
		"@types/glob": "^7.1.1",
		"@types/node": "^12.0.8",
		"@types/node-fetch": "^2.5.5",
		"@types/path-to-regexp": "^1.7.0",
		"@types/ping": "^0.2.0",
		"@types/react": "^17.0.6",
		"@types/react-dom": "^17.0.5",
		"@types/serialport": "^8.0.0",
		"@types/serve-static": "^1.13.2",
		"@types/ws": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"antd": "^4.15.6",
		"esbuild": "^0.12.1",
		"eslint": "^7.27.0",
		"eslint-plugin-node": "^11.1.0",
		"gts": "^3.1.0",
		"html-typings": "^1.1.4",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.0",
		"pretty-quick": "^2.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rollup": "^1.20.3",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@googleapis/homegraph": "^2.0.0",
		"@pm2/io": "^5.0.0",
		"@pm2/node-runtime-stats": "^0.3.2",
		"@sanderronde/write-buffer": "1.0.3",
		"@serialport/parser-readline": "^9.0.7",
		"actions-on-google": "^2.14.0",
		"body-parser": "^1.19.0",
		"castv2-player": "^2.1.0",
		"chalk": "^2.4.2",
		"cookie-parser": "^1.4.4",
		"dotenv": "^8.2.0",
		"ewelink-api": "^3.1.1",
		"express": "^4.17.1",
		"express-oauth-server": "^2.0.0",
		"fs-extra": "^8.0.1",
		"glob": "^7.1.6",
		"google-auth-library": "^7.0.4",
		"googleapis": "^73.0.0",
		"lit-html": "^1.1.0",
		"magic-home": "^2.6.0",
		"node-fetch": "^2.6.0",
		"oauth2-server": "^3.1.1",
		"optional-require": "^1.0.0",
		"ping": "^0.2.2",
		"serialport": "^8.0.7",
		"serve-static": "^1.14.1",
		"telnet-client": "^1.2.5",
		"ts-node": "^10.0.0",
		"uglify-es": "^3.3.9",
		"wc-lib": "^1.1.36",
		"ws": "^7.4.6"
	}
}
