		"node": ">=16.0.0"
	},
	"scripts": {
		"postinstall": "patch-package",
		"compile": "tsgo -p app/tsconfig.json",
		"watch": "tsgo -w -p app/tsconfig.json",
		"pretty": "pretty-quick",
		"format-check": "prettier --check --experimental-cli '**/*.{ts,tsx,json,html}'",
		"format": "prettier --write --experimental-cli '**/*.{ts,tsx,json,html}'",
		"lint": "bun format && bun compile && bun node_modules/oxlint/dist/cli.js app scripts",
		"lint:fix": "bun node_modules/oxlint/dist/cli.js --fix app scripts",
		"test": "bun test app/server app/client/lib app/client/dashboard",
		"test:watch": "bun test --watch app/server app/client/lib app/client/dashboard",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:all": "bun test && playwright test",
		"start:prod": "NODE_ENV=production bun app/server/app.ts --http 1272 --https 1273 -vvvvvv --color -i",
		"dev": "bun app/server/app.ts --http 1272 --https 1273 -vvvvvv --color -i --debug",
		"knip": "knip-bun"
	},
