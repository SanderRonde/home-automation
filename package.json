{
	"name": "home-automation",
	"version": "1.0.0",
	"main": "app/server/app.js",
	"repository": "https://github.com/SanderRonde/home-automation",
	"author": "Sander Ronde <awsdfgvhbjn@gmail.com>",
	"license": "MIT",
	"private": true,
	"nodemonConfig": {
		"ext": "js",
		"exec": "node --experimental-modules --enable-source-maps --trace-warnings -r ts-node/register/transpile-only app/server/app.ts --http 1272 --https 1273 -v* --log-secrets",
		"watch": [
			"app/server/"
		]
	},
	"workspaces": [
		"serial_mon"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"compile:server": "tsc -p app/server/tsconfig.json",
		"compile:client": "tsc -p app/client/tsconfig.json",
		"compile:ai": "tsc -p ai/tsconfig.json",
		"bundle:keyval": "bun build app/client/keyval/keyval.tsx --outdir app/client/keyval && bun build app/client/keyval/serviceworker.ts --outfile=app/client/keyval/serviceworker.js",
		"bundle:config": "bun build app/client/config/config.tsx --outdir app/client/config",
		"bundle:home-detector": "bun build app/client/home-detector/home-detector.ts --outfile=app/client/home-detector/home-detector.bundle.js && bun build app/client/home-detector/serviceworker.ts --outfile=app/client/home-detector/serviceworker.bundle.js",
		"bundle:info-screen": "bun build app/client/info-screen/info-screen.ts --outfile=app/client/info-screen/info-screen.bundle.js",
		"bundle:oauth": "bun build app/client/oauth/oauth.tsx --outfile=app/client/oauth/oauth.bundle.js",
		"bundle": "bun bundle:keyval && bun bundle:config && bun bundle:home-detector && bun bundle:info-screen && bun bundle:oauth",
		"watch:server": "tsc -w -p app/server/tsconfig.json",
		"watch:client": "tsc -w -p app/client/tsconfig.json",
		"watch:ai": "tsc -w -p ai/tsconfig.json",
		"compile": "bun compile:server && bun compile:client && bun compile:ai && bun bundle",
		"watch": "tsc -w -p app/server/tsconfig.json",
		"pretty": "pretty-quick",
		"format-check": "prettier --check '**/*.{ts,json,html}'",
		"format": "prettier --write '**/*.{ts,json,html}'",
		"lint": "bun format-check && bun compile:server && eslint --fix \"./app/**/*.ts\" && bun test",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"start": "bun app/server/app.ts --http 1272 --https 1273 -vvvvvv --color -i"
	},
	"devDependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@matter/types": "^0.15.3",
		"@monaco-editor/react": "^4.7.0",
		"@mui/icons-material": "^7.3.1",
		"@mui/material": "^7.3.1",
		"@types/async-lock": "^1",
		"@types/bun": "^1.1.14",
		"@types/chalk": "^2.2.0",
		"@types/cookie-parser": "^1.4.1",
		"@types/express": "^4.17.0",
		"@types/express-oauth-server": "^2.0.3",
		"@types/fs-extra": "^7.0.0",
		"@types/minimatch": "5",
		"@types/node": "20",
		"@types/node-fetch": "^2.5.5",
		"@types/path-to-regexp": "^1.7.0",
		"@types/ping": "^0.2.0",
		"@types/react": "^18.3.2",
