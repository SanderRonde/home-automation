  "license": "MIT",
  "private": true,
  "nodemonConfig": {
    "ext": "js",
    "exec": "node app/server/app.js --http 1272 --https 1273 -vv --log-secrets",
    "watch": [
      "app/server/"
    ]
  },
  "scripts": {
    "compile:server": "tsc -p app/server/tsconfig.json",
    "compile:client": "tsc -p app/client/tsconfig.json",
    "bundle": "rollup --config rollup.config.js",
    "watch:server": "tsc -w -p app/server/tsconfig.json",
    "watch:client": "tsc -w -p app/client/tsconfig.json",
    "html-typings": "",
    "compile": "npm run compile:server && npm run compile:client && npm run bundle"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/node": "^12.0.8",
    "@types/path-to-regexp": "^1.7.0",
    "@types/ping": "^0.2.0",
    "@types/serialport": "^7.0.4",
    "@types/serve-static": "^1.13.2",
    "@types/ws": "^6.0.3",
    "html-typings": "^1.1.3",
    "rollup": "^1.20.3",
    "rollup-plugin-uglify-es": "^0.0.1",
    "typescript": "^3.7.1-rc"
  },
  "dependencies": {
    "@serialport/parser-readline": "^2.0.2",
    "body-parser": "^1.19.0",
    "castv2-player": "^2.1.0",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.6",
    "lit-html": "^1.1.0",
    "magic-home": "^2.1.1",
    "path-to-regexp": "^3.0.0",
    "ping": "^0.2.2",
    "serialport": "^7.1.5",
    "serve-static": "^1.14.1",
    "wc-lib": "^1.0.5",
    "ws": "^7.2.0"
  }
}
