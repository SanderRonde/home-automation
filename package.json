{
	"name": "home-automation",
	"version": "1.0.0",
	"main": "app/server/app.js",
	"repository": "https://github.com/SanderRonde/home-automation",
	"author": "Sander Ronde <awsdfgvhbjn@gmail.com>",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"postinstall": "patch-package",
		"compile": "tsc -p app/tsconfig.json",
		"watch": "tsc -w -p app/tsconfig.json",
		"pretty": "pretty-quick",
		"format-check": "prettier --check '**/*.{ts,tsx,json,html}'",
		"format": "prettier --write '**/*.{ts,tsx,json,html}'",
		"lint": "bun format && bun compile && eslint \"./{app,scripts}/**/*.{ts,tsx}\"",
		"eslint:fix": "eslint --fix \"./{app,scripts}/**/*.{ts,tsx}\"",
		"test": "bun test app/server app/client/lib app/client/dashboard",
		"test:watch": "bun test --watch app/server app/client/lib app/client/dashboard",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"test:all": "bun test && playwright test",
		"start:prod": "NODE_ENV=production bun app/server/app.ts --http 1272 --https 1273 -vvvvvv --color -i",
		"dev": "bun app/server/app.ts --http 1272 --https 1273 -vvvvvv --color -i --debug",
		"knip": "knip-bun"
	},
	"devDependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@eslint/compat": "^1.4.0",
		"@matter/types": "^0.15.3",
		"@playwright/test": "^1.56.0",
		"@types/bun": "^1.1.14",
		"@types/fs-extra": "^7.0.0",
		"@types/node": "^20.19.11",
		"@types/ping": "^0.2.0",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"eslint": "8",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-unused-imports": "^3.2.0",
		"gts": "^6.0.2",
		"knip": "^5.64.1",
		"playwright": "^1.56.0",
		"prettier": "^3.2.5",
		"prettier-plugin-sort-imports": "^1.8.5",
		"pretty-quick": "^4.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@matter/main": "^0.15.3",
		"@modelcontextprotocol/sdk": "^1.18.2",
		"@mui/icons-material": "^7.3.2",
		"@mui/material": "^7.3.2",
		"@project-chip/matter.js": "^0.15.3",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@uiw/react-color": "^2.9.0",
		"chalk": "^2.4.2",
		"chart.js": "^4.5.1",
		"cie-rgb-color-converter": "^1.0.6",
		"dotenv": "^17.2.1",
		"ewelink-api-next": "^1.0.4",
		"framer-motion": "^12.23.24",
		"fs-extra": "^8.0.1",
		"glob": "^11.0.3",
		"google-auth-library": "^7.0.4",
		"googleapis": "^73.0.0",
		"happy-dom": "^20.0.0",
		"lit-html": "1.1.0",
		"magic-home": "^2.6.0",
		"optional-require": "^1.0.0",
